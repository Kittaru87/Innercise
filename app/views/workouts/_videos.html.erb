
<header class="md-4">
  <h2 class="text-center"><%= id.capitalize %> workouts</h2>
</header>
<section>
  <% workouts.each do |workout| %>
    <div class="video-div">
      <h4><%= workout["title"] %></h4>
      </br>
      <p><%= workout["description"] %></p>
      </br>
      <p>- <%= workout["channel"] %></p>
      </br>
      <iframe width="560" height="315" src="https://www.youtube.com/embed/<%=workout["videoId"]%>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    </div>
  <% end %>
</section>