
<header>
  <h2><%= id %> workouts</h2>
</header>
<section>
  <% workouts.each do |workout| %>
    <div>
      <h4><%= workout["title"] %></h4>
      </br>
      <p><%= workout["description"] %></p>
      </br>
      <p><%= workout["channel"] %></p>
      </br>
      <iframe width="560" height="315" src="https://www.youtube.com/embed/"+<%=workout["id"]%> frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    </div>
  <% end %>
</section>