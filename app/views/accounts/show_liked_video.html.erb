
  <div class="container">
    <div class="row justify-content-between">
      <div class="mt-4">
        <%= form_with url: profile_path(@body_id), method: :put do |form| %>
          <%= hidden_field_tag(:nextPageToken, @workouts[0]['prevPageToken']) %>
          <%= form.submit "Previous", class: "btn btn-default btn-primary ml-3" %>
        <% end %>
      </div>
      <div class="mt-4">
        <%= form_with url: profile_path(@body_id), method: :put do |form| %>
          <%= hidden_field_tag(:nextPageToken, @workouts[0]['nextPageToken']) %>
          <%= form.submit "Next", class: "btn btn-default btn-primary mr-3" %>
        <% end %>
      </div>
    </div>
  </div>

  <% workouts.each do |workout| %>
    <div class="video-div col-lg-8 text-center" style="margin-bottom: 3em;">
      <h4><%= workout["title"] %></h4>
      </br>
      <p><%= workout["description"] %></p>
      <p>- <%= workout["channel"] %></p>
      </br>
      <div class="embed-responsive embed-responsive-16by9">
        <iframe class="embed-responsive-item" style="padding: 5px;" width="560" height="315" src="https://www.youtube.com/embed/<%=workout["videoId"]%>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
      </div>
    </div>
  <% end %>
