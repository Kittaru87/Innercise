<div class="border py-4 px-3">
<h3 class="text-center" style="margin-block-end: 1em;">Most Popular</h3>

<% top_5 = Like.popular %>

<% top_5.each do |like| %>
  <%= form_with url: profile_like_path(like.workout.bodyId), method: :put do |form| %>
    <%= hidden_field_tag(:videoId, like.workout.videoId) %>
    <%= hidden_field_tag(:id, like.workout.bodyId) %>
    <%= form.submit like.workout.title, class: "liked-videos" %>
  <% end %>
  <br />
  <p>*******</p>
<% end %>
</div>

